<%= form_for @skate_ostalo,:url => skate_ostalos_path, :html => { :multipart => true } do |d| %>
  <%= render 'shared/error_messages', object: d.object %>
  <div class="field" >
        <%= d.label :naslov %>
        <%= d.text_field :naslov %>

        <%= d.label :opis, "Opis:" %>
        <%= d.text_area :opis %>

        <%= d.label :cijena %>
        <%= d.text_field :cijena %>

        <br/>
        <%= d.file_field :slika %>
  </div>
  <%= d.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>


